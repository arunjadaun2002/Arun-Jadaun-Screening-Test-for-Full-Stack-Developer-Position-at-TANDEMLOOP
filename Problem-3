#include<bits/stdc++.h>
#include <iostream>
using namespace std;


void fun(int n){
    int odd=1;
    while(n){
        cout<<odd<<" ";
        odd+=2;
        n--;
        
    }
}
int main()
{
    int a;
    cout<<"Enter the number "<<endl;
    cin>>a;
    
    
    if(a<=0){
        cout<<"Please enter the positive number ";
    }
    
    else if(a%2==0){
        a-=1;
        fun(a);
    }
    else if(a%2!=0)
    fun(a);
  
    
     

    return 0;
}
